@inherits UmbracoTemplatePage<BlogIndex>

@using NicBell.UCreate.Test.DocumentTypes

@{
    Layout = "_Layout.cshtml";
    ViewBag.Title = Model.Content.MetaTitle;
}


<div class="jumbotron">
    <h1>@Model.Content.Name</h1>
</div>
<ul>
    @foreach (var page in Model.Content.Children<BlogItem>())
    {
        <li><a href="@page.Url">@page.ItemTitle</a></li>
    }
</ul>
